               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <TextArea fx:id="textLecture" layoutX="330.0" layoutY="50.0" prefHeight="367.0" prefWidth="554.0" style="-fx-background-color: #ffffff; -fx-border-color: #000000; -fx-border-radius: 5;" />
            <Button fx:id="newLectureButton" layoutX="709.0" layoutY="436.0" mnemonicParsing="false" onAction="#newLecture" prefHeight="50.0" prefWidth="175.0" style="-fx-background-color: #ffffff; -fx-border-color: #000000; -fx-border-radius: 5;" text="Добавить лекцию">
               <font>
                  <Font size="18.0" />
